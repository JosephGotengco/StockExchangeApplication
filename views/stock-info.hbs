<!-- ChartJS CDN 4 -->
<script src="https://cdnjs.cloudflare.com/ajax/libs/Chart.js/2.8.0/Chart.min.js" crossorigin="anonymous"></script>


<script>


	// Any of the following formats may be used
	var ctx = document.getElementById('myChart');
	var myColors = ['red', 'green', 'blue'];
	var myChart = new Chart(ctx, {
		type: 'line',
		data: {
			labels: {{{ json labels }}},
	datasets: [{
		label: '{{chart_title}} (compared to {{{currency_code}}})',
		data: {{{ json data }}},
		backgroundColor: [
		'rgba(255, 99, 132, 0.2)',
		'rgba(54, 162, 235, 0.2)',
		'rgba(255, 206, 86, 0.2)',
		'rgba(75, 192, 192, 0.2)',
		'rgba(153, 102, 255, 0.2)',
		'rgba(255, 159, 64, 0.2)'
	],
		borderColor: [
		'rgba(255, 99, 132, 1)',
		'rgba(54, 162, 235, 1)',
		'rgba(255, 206, 86, 1)',
		'rgba(75, 192, 192, 1)',
		'rgba(153, 102, 255, 1)',
		'rgba(255, 159, 64, 1)'
	],
		fill: true,
		borderWidth: 1,
		pointHitRadius: 1,
		borderCapStyle: 'miter',
		tension: 0
			}]
		},
	options: {
		scales: {
			yAxes: [{
				ticks: {
					beginAtZero: false
				},
				scaleLabel: {
					display: true,
					labelString: '{{{currency_code}}} ({{{currency_symbol}}})'
				}
			}],
				xAxes: [{
					scaleLabel: {
						display: true,
						labelString: 'Date (yyyy-mm--dd)'
					}
				}]
		}
	}
	});
</script>

