<style>
	@import url('https://fonts.googleapis.com/css?family=Quantico');

	* {
		box-sizing: border-box;
	}

	html,
	body {
		margin: 0;
		padding: 0;
		font-size: 1rem;
		line-height: 1.5;
		height: 100%;
		font-family: 'Quantico', sans-serif;
		overflow-x: hidden;
	}

	.content-margin {
		margin-top: 75px;
	}

	.nav-margin {
		margin-bottom: 75px;
	}

	.nav-margin-trading {
		margin-bottom: 35px;
	}

	.title {
		text-align: center;
		font-size: 20px;
	}



	.footer-margin {
		height: 150px;
	}

	.copyright {
		color: white;
		font-size: 12px;
	}


	.content {
		display: flex;
		height: 65vh;
		align-items: center;
		justify-content: center;
		text-align: center;
	}


	.not-login-msg {
		width: 50%;
	}

	.nav-color,
	.footer {
		background-color: #272727;
	}



	.footer-spacer {
		margin-bottom: 100px;
	}
</style>
<body>

	<!-- NAV BAR -->
    <div class="d-flex flex-column flex-md-row align-items-center p-3 px-md-4 mb-3 border-bottom shadow-sm nav-color">
